#version 330 core

uniform bool prune_empty;

uniform bool with_interpolated_holes;

in vec4 color_fs;
in float discard_fs;

out vec4 color;


void main()
{
	if (prune_empty && ((discard_fs > 0 && !with_interpolated_holes) || (discard_fs == 1 && with_interpolated_holes)))
		discard; // don't render "empty" geometry
	color = color_fs;
}